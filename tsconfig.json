{
  "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "suppressImplicitAnyIndexErrors": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "resolveJsonModule": true,
      "lib": [
          "esnext",
          "dom"
      ],
      "types": [
          "vite/client",
          "node"
      ],
      "baseUrl": ".",
      "paths": {
          "@/*": [
              "src/*"
          ]
      }
  },
  "include": [
      "src/**/*.ts",
      "src/**/*.d.ts",
      "src/**/*.tsx",
      "src/**/*.vue",
      "src/views/gridConfig/GridConfig.vue",
      "*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
